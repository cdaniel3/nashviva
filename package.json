    "fixcode": "standard --fix",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-for-nashville/nashviva.git"
  },
  "author": "Code For Nashville",
  "bugs": {
    "url": "https://github.com/code-for-nashville/nashviva/issues"
  },
  "homepage": "https://github.com/code-for-nashville/nashviva#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "fs": "0.0.1-security",
    "husky": "^0.13.4",
    "sass-loader": "^6.0.6",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
